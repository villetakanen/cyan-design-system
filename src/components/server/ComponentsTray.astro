---
import { getCollection } from 'astro:content';

const allComponents = await getCollection('elements');
---

<h3 class="cn-tray-title">Web Components</h3>
<ul>
  {
    allComponents.map(p => (
      <li>
        <a href={'/docs/fi/' + p.slug}>
          {p.data.noun && <cn-icon noun={p.data.noun} small></cn-icon>}
          {p.data.shortname || p.data.name}</a>
      </li>
    ))
  }
</ul>